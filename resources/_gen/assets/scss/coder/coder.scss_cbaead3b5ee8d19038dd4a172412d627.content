.blogpost {
  counter-reset: section; }
  .blogpost ul li:before {
    content: none; }
  .blogpost h1 {
    text-decoration: underline dashed var(--titlecolor) 2px;
    padding-left: 1%;
    counter-reset: sub-section; }
  .blogpost h2 {
    text-decoration: underline dashed var(--titlecolor) 2px;
    padding-left: 2%;
    counter-reset: composite; }
  .blogpost h3 {
    text-decoration: underline dashed var(--titlecolor) 2px;
    padding-left: 3%;
    counter-reset: detail; }
  .blogpost h4 {
    text-decoration: underline dashed var(--titlecolor) 2px;
    padding-left: 4%;
    counter-reset: list; }
  .blogpost h5 {
    text-decoration: underline dashed var(--titlecolor) 2px;
    padding-left: 5%;
    counter-reset: point; }
  .blogpost h6 {
    text-decoration: underline dashed var(--titlecolor) 2px;
    padding-left: 6%; }
  .blogpost h1:before {
    counter-increment: section;
    content: counter(section) " "; }
  .blogpost h2:before {
    counter-increment: sub-section;
    content: counter(section) "." counter(sub-section) " "; }
  .blogpost h3:before {
    counter-increment: composite;
    content: counter(section) "." counter(sub-section) "." counter(composite) " "; }
  .blogpost h4:before {
    counter-increment: detail;
    content: counter(section) "." counter(sub-section) "." counter(composite) "." counter(detail) " "; }
  .blogpost h5:before {
    counter-increment: list;
    content: counter(section) "." counter(sub-section) "." counter(composite) "." counter(detail) "." counter(list) " "; }
  .blogpost h6:before {
    counter-increment: point;
    content: counter(section) "." counter(sub-section) "." counter(composite) "." counter(detail) "." counter(list) "." counter(point) " "; }

